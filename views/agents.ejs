<h1 class="pb-4">Explore Agents</h1>

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/">Home</a></li>
      <li class="breadcrumb-item active" aria-current="page">Explore Agents</li>
    </ol>
  </nav>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3 mb-4">
    <% if(elements != '') { %>
        <% elements.forEach(function(element, index) { %>
            <a href="/agent/<%= element.uuid %>" class="col text-center category__link">
                <div class="category__img shadow">
                    <img src="<%- element.fullPortrait %>" alt="<%- element.displayName %>" style="transform: scale(1.2);" loading="lazy">
                </div>
                <div class="pt-1"><%- element.displayName %></div>
            </a>
        <% }) %>
    <% } %>
</div>  
 